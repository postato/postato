{
  "name": "postato",
  "version": "1.0.0",
  "description": "Clean starter template for generating TypeScript API tests from Postman collections using @postato/shared CLI",
  "main": "index.js",
  "scripts": {
    "generate:base": "postato-generate",
    "test": "jest",
    "test:dev": "TEST_ENV=dev jest",
    "test:staging": "TEST_ENV=staging jest",
    "test:prod": "TEST_ENV=prod jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "api-testing",
    "supertest",
    "typescript",
    "jest",
    "postman",
    "test-automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@postato/shared": "^0.2.10",
    "dotenv": "^16.4.5",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.0"
  }
}
