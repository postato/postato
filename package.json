{
  "name": "api-testing-template",
  "version": "1.0.0",
  "description": "TypeScript + SuperTest API testing template that converts Postman collections into version-controlled test suites",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:dev": "TEST_ENV=dev jest",
    "test:staging": "TEST_ENV=staging jest",
    "test:prod": "TEST_ENV=prod jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "api-testing",
    "supertest",
    "typescript",
    "jest",
    "postman",
    "test-automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "supertest": "^7.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  }
}
